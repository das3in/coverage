<h1>Edit <%= @team.display_name %></h1>
<%= simple_form_for(@team) do |f| %>
  <div class="form-group">
    <%= f.input :location, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.input :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.file_field :avatar %>
  </div>
  <div class="form-group">
    <%= f.submit "Submit", class: "btn btn-success" %>
  </div>
<% end %>
